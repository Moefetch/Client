{
  "name": "moefetch-client",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --host --port 2345",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "deploy": "deploy.sh"
  },
  "dependencies": {
    "@vueuse/core": "^6.9.2",
    "nanoid": "^3.3.0",
    "vue": "^3.2.31",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@types/node": "^17.0.19",
    "@vitejs/plugin-vue": "^1.10.2",
    "typescript": "^4.5.5",
    "vite": "^2.9.13",
    "vite-plugin-windicss": "^1.7.0",
    "vue-tsc": "^0.3.0",
    "windicss": "^3.4.3"
  },
  "pkg": {
    "scripts": ["./src/Logic/*.js", "./src/Logic/LogicCategories/*.js", "./src/Logic/LogicCategories/*/*.js", "./src/Logic/LogicCategories/*/*/*.js", "./src/Logic/LogicCategories/*/*/*/*.js"],
    "assets": ["./src/Logic/*.js", "./src/Logic/LogicCategories/*.js", "./src/Logic/LogicCategories/*/*.js", "./src/Logic/LogicCategories/*/*/*.js", "./src/Logic/LogicCategories/*/*/*/*.js"],
    "targets": [ "latest-linux-x64", "latest-win-x64" ],
    "outputPath": "dist"
  },
  "bin": "./index.js"
}
