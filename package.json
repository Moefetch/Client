{
  "name": "moefetch-client",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --host --port 2345",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "deploy": "deploy.sh"
  },
  "dependencies": {
    "@vueuse/core": "^6.9.2",
    "i": "^0.3.7",
    "nanoid": "^3.3.0",
    "npm": "^10.8.2",
    "vue": "^3.2.31",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@types/node": "^17.0.19",
    "@vitejs/plugin-vue": "^5.2.0",
    "typescript": "^4.5.5",
    "vite": "^5.4.11",
    "vite-plugin-windicss": "^1.7.0",
    "vue-tsc": "^0.3.0",
    "windicss": "^3.4.3"
  },
  "pkg": {
    "scripts": [
      "./src/Logic/*.js",
      "./src/Logic/LogicCategories/*.js",
      "./src/Logic/LogicCategories/*/*.js",
      "./src/Logic/LogicCategories/*/*/*.js",
      "./src/Logic/LogicCategories/*/*/*/*.js"
    ],
    "assets": [
      "./src/Logic/*.js",
      "./src/Logic/LogicCategories/*.js",
      "./src/Logic/LogicCategories/*/*.js",
      "./src/Logic/LogicCategories/*/*/*.js",
      "./src/Logic/LogicCategories/*/*/*/*.js"
    ],
    "targets": [
      "latest-linux-x64",
      "latest-win-x64"
    ],
    "outputPath": "dist"
  },
  "bin": "./index.js"
}
